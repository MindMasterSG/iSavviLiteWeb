<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>@ViewBag.Title</title>
    @*<link href="@Url.Content("~Content/css/etrac.css")" rel="stylesheet" type="text/css" />*@
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>
    <link href="../../Content/css/etrac.css" rel="stylesheet" type="text/css" />
</head>
<body>
    <div class="header_part">
        <div class="header_wrapper">
            <div class="header_main_mt_cont">
                <div class="logo">
                    <a href="index.html">
                        <img src="../../Content/images/logo.png" alt="" border="0" /></a></div>
                <div class="top_menu">
                    Wellcome :
                    <li><a href="gallery.html" class="footer_Highlight">@Session["UserName"].ToString()</a></li>
                    <li>| </li>
                    <li>@Html.ActionLink("Logout", "Logout", "SMSAdmin")</li></div>
            </div>
            <div class="menu_cont">
                <div class="check_in_button">
                    <a href="#"></a>
                </div>
                <div class="vehicles_button">
                    <a href="#"></a>
                </div>
                <div class="occurrences_button">
                    <a href="#"></a>
                </div>
                <div class="incidents_button">
                    @Html.ActionLink("Incidents", "GetIncidents", "SMSAdmin")
                </div>
                <div class="event_button">
                    <a href="#"></a>
                </div>
                <div class="lost_button">
                    @Html.ActionLink("Lost And Found", "GetLostAndFound", "SMSAdmin")
                </div>
            </div>

            @RenderBody()
            <div class="bottom_line">
            </div>
            <div class="coperight_cont_txt">
                Copyright 2012 © SpinnetAsia Pte. Ltd.</div>
        </div>
    </div>
</body>
</html>
