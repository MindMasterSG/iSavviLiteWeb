@model SMSEntities.SMSDBEntities.UserTypes

<h2>Create User Type</h2>
<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

@using (Html.BeginForm())
{
    @Html.ValidationSummary(true, "Please correct the below errors.")
    <legend>Create User type</legend>
    <fieldset><div class="check_main_bg_cont">
    <div class="check_left_cont_txt">
        <div class="editor-label" >
            <legend>User Type</legend>
        </div>
        <div class="editor-field">
             @Html.TextBoxFor(model => model.UserType, new { @class = "bigtextfield" })
             @Html.ValidationMessageFor(model => model.UserType)
        </div>
        <div class="editor-label" >
            <legend>Description</legend>
        </div>
        <div class="editor-field">
             @Html.TextBoxFor(model => model.Description, new { @class = "bigtextfield" })
             @*@Html.ValidationMessageFor(model => model.LoginID)*@
        </div>
        <div class="editor-label" >
            <legend>Level</legend>
        </div>
        <div class="editor-field">
             @Html.TextBoxFor(model => model.Level, new { @class = "bigtextfield" })
             @*@Html.ValidationMessageFor(model => model.Level)*@
        </div>
        <div class="editor-label" >
            <legend>Parent</legend>
        </div>
        <div class="editor-field">
             @Html.TextBoxFor(model => model.Parent, new { @class = "bigtextfield" })
        </div>
        </div>
        <div style="margin-left: 120px;">
            <input type="submit" class="btnAddUpdate" value="Add" />
        </div>
        </div>
    </fieldset>
}

